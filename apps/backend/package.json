{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node -r ts-node/register src/index.ts",
    "dev:watch": "nodemon",
    "build": "rimraf dist && tsc",
    "start:production": "node dist/index.js",
    "lint": "eslint {**/*,*}.{js,ts,jsx,tsx} --ignore-path ../../.gitignore",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prisma:generate": "prisma generate",
    "test": "vitest",
    "db:test:push": "dotenv -e .env.test -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:status": "dotenv -e .env.local -- prisma migrate status",
    "db:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:seed": "dotenv -e .env.local -- ts-node prisma/seeds/seeds.ts"
  },
  "dependencies": {
    "@fastify/auth": "^4.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/jwt": "^6.6.0",
    "@fastify/static": "^6.9.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.4.0",
    "@prisma/client": "^4.10.1",
    "common-src": "*",
    "dockerode": "^3.3.4",
    "dotenv": "^16.0.3",
    "fastify": "^4.13.0",
    "fastify-bcrypt": "^1.0.1",
    "fastify-plugin": "^4.5.0",
    "json-schema-to-ts": "^2.7.2",
    "node-fetch": "^2.6.9",
    "recursive-readdir": "^2.2.3",
    "socket.io": "^4.6.1",
    "ts-node": "^10.9.1",
    "unzip-stream": "^0.3.1",
    "wtfnode": "^0.9.1"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.14",
    "@types/node": "^18.13.0",
    "@types/node-fetch": "^2.6.2",
    "@types/recursive-readdir": "^2.2.1",
    "@types/unzip-stream": "^0.3.1",
    "@types/wtfnode": "^0.7.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.3.0",
    "prisma": "^4.10.1",
    "tsconfig": "*",
    "typescript": "^4.5.3",
    "vitest": "^0.29.2"
  }
}
